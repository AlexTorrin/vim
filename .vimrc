execute pathogen#infect()
syntax on
filetype plugin indent on
map ; :Files<CR>
map ' :Ag<CR>
set number
set shiftwidth=4 softtabstop=4 expandtab
map <C-o> :NERDTreeToggle<CR>
map <C-i> :NERDTreeFind<CR>
map <C-l> :tabNext<CR>
map <C-h> :tabprevious<CR>
map <C-k> :tabnew<CR>
map <C-j> :tabclose<CR>

